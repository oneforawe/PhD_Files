#filename: plot_2Dvpt_constT_lmax100_dl0.001_Op1_GammaVsr.gnu
reset
set terminal postscript color eps enhanced
set output 'plot_2Dvpt_constT_lmax100_dl0.001_Op1_GammaVsr.ps'
set size 0.85,0.85

set title "^4He Vortex-Pair Distribution {/Symbol-Oblique G} (times {/Times-Italic a}_0^4) vs Pair Separation Parameter {/Times-Italic l}"

set ylabel "{/Symbol-Oblique G} {/Times-Italic a}_0^4 (unitless)"
set xlabel "{/Times-Italic l} (unitless)"
#set xrange [0:0.005]
#set xrange [0:0.02]
#set yrange [1e-20:1]
set yrange [1e-200:1]
set logscale y
set grid
#set key outside right box width -20 height 0.5 spacing 1.2 title " Calculated from {/CM-Typewriter vlt\\_ThermStates.c} \n Fit: {/Symbol-Oblique r}_s / {/Symbol-Oblique r} = {/Times-Italic A}{/Symbol \242}{/Symbol-Oblique t}^{/Symbol-Oblique n} \n {/Symbol-Oblique n} = 0.6716883522798452"
set key inside top right box width -2 height 0.5 spacing 1.2 title " ^{}Vortex Pair Theory "
#set rmargin 35

plot '2Dvpt_constT_1_lmax100_dl0.001_Op1.dat'   using 3:7 with lines lw 2 title " {/Times-Italic T} = 1.0*{/Times-Italic T}_{KT} ", \
     '2Dvpt_constT_0.9_lmax100_dl0.001_Op1.dat' using 3:7 with lines lw 2 title " 0.9*{/Times-Italic T}_{KT} ", \
     '2Dvpt_constT_0.8_lmax100_dl0.001_Op1.dat' using 3:7 with lines lw 2 title " 0.8*{/Times-Italic T}_{KT} ", \
     '2Dvpt_constT_0.7_lmax100_dl0.001_Op1.dat' using 3:7 with lines lw 2 title " 0.7*{/Times-Italic T}_{KT} ", \
     '2Dvpt_constT_0.6_lmax100_dl0.001_Op1.dat' using 3:7 with lines lw 2 title " 0.6*{/Times-Italic T}_{KT} ", \
     '2Dvpt_constT_0.5_lmax100_dl0.001_Op1.dat' using 3:7 with lines lw 2 title " 0.5*{/Times-Italic T}_{KT} ", \
     '2Dvpt_constT_0.4_lmax100_dl0.001_Op1.dat' using 3:7 with lines lw 2 title " 0.4*{/Times-Italic T}_{KT} ", \
