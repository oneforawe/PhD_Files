#filename: fit_Pdep_K0c.gnu
reset
fK0c(x)  = k00 + k01*x + k02*x**2 + k03*x**3 + k04*x**4 + k05*x**5 + k06*x**6 + k07*x**7 + k08*x**8 + k09*x**9 + k10*x**10
fit fK0c(x) 'Pdep_K0c_lmax100_dl0.001_Op2.dat' using 3:1 via k00,k01,k02,k03,k04,k05,k06,k07,k08,k09,k10
update 'fit_Pdep_K0c.par' 'fit_Pdep_K0c.par'
set print 'fit_Pdep_K0c.par'
print "#filename: fit_Pdep_K0c.par\n\n", \
      "K0c(P) fit parameters\n", \
      k00,"\n", \
      k01,"\n", \
      k02,"\n", \
      k03,"\n", \
      k04,"\n", \
      k05,"\n", \
      k06,"\n", \
      k07,"\n", \
      k08,"\n", \
      k09,"\n", \
      k10
unset print
set print 'fit_Pdep_K0c_results.dat'
print "filename: fit_Pdep_K0c_results.dat\n\n", \
      "K0c(P) fit parameters\n", \
      k00,"\n", \
      k01,"\n", \
      k02,"\n", \
      k03,"\n", \
      k04,"\n", \
      k05,"\n", \
      k06,"\n", \
      k07,"\n", \
      k08,"\n", \
      k09,"\n", \
      k10,"\n\n\n", \
      "If you want to plot the fit in gnuplot (with plot_fit_Pdep_K0c.gnu), use this:\n", \
      "#Defining the fit parameters and equation:\n", \
      "k00 = ",k00,"\n", \
      "k01 = ",k01,"\n", \
      "k02 = ",k02,"\n", \
      "k03 = ",k03,"\n", \
      "k04 = ",k04,"\n", \
      "k05 = ",k05,"\n", \
      "k06 = ",k06,"\n", \
      "k07 = ",k07,"\n", \
      "k08 = ",k08,"\n", \
      "k09 = ",k09,"\n", \
      "k10 = ",k10,"\n", \
      "fK0c(x)  = k00 + k01*x + k02*x**2 + k03*x**3 + k04*x**4 + k05*x**5 + k06*x**6 + k07*x**7 + k08*x**8 + k09*x**9 + k10*x**10\n\n\n", \
      "So we have...\n", \
      "P (bar)\t","K0c(P)\n", \
      "0.050\t",fK0c(0.050),"\n", \
      "1.646\t",fK0c(1.646),"\n", \
      "7.328\t",fK0c(7.328),"\n", \
      "15.031\t",fK0c(15.031),"\n", \
      "18.180\t",fK0c(18.180),"\n", \
      "22.533\t",fK0c(22.533),"\n", \
      "25.868\t",fK0c(25.868),"\n\n", \
      "P (bar=atm*1.01325)\tP (atm)\t","K0c(P)\n", \
      "0.0*1.01325\t0.0\t",fK0c(0.0*1.01325),"\n", \
      "1.0*1.01325\t1.0\t",fK0c(1.0*1.01325),"\n", \
      "5.0*1.01325\t5.0\t",fK0c(5.0*1.01325),"\n", \
